<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Entity\Product" table="product">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prix" type="integer" column="prix" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="valide" type="string" column="valide" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="stock" type="integer" column="stock" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-many field="panier" target-entity="Panier" mapped-by="product" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
